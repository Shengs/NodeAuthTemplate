extends layout

block content
	h1 Login
	p Please login using the form below
	form(method='post',action='/users/login',enctype='multipart/form-data')
		.form-group
			lable Username
			input.form-control(name='username', type='text',placeholder='Your username')
		.form-group
			lable Password
			input.form-control(name='password', type='password',placeholder='Enter Your Password')
		input.btn.btn-default(name='submit',type='submit',value='Login')